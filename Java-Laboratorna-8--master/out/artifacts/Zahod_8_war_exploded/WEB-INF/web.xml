<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>foo.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/MyServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddBasketServlet</servlet-name>
        <servlet-class>foo.AddBasketServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddBasketServlet</servlet-name>
        <url-pattern>/AddBasketServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UpdateBasketServlet</servlet-name>
        <servlet-class>foo.UpdateBasketServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateBasketServlet</servlet-name>
        <url-pattern>/UpdateBasketServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UpdateProductServlet</servlet-name>
        <servlet-class>foo.UpdateProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateProductServlet</servlet-name>
        <url-pattern>/UpdateProductServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddProductServlet</servlet-name>
        <servlet-class>foo.AddProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddProductServlet</servlet-name>
        <url-pattern>/AddProductServlet</url-pattern>
    </servlet-mapping>
</web-app>